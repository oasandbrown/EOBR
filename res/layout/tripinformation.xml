<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:id="@+id/relativeLayout2" 
    			android:layout_width="fill_parent" 
    			android:gravity="left" 
    			android:layout_height="600px">	
 
 <!--%&%&%&%&%&%&%&%&%&%&%&%&%&%&%& ORIGIN DELAYS %&%&%&%&%&%&%&%&%&%&%&%&%%&%%&%&%&%& -->
        
         	<TextView
    			android:id="@+id/textview3"
        		android:text="Retrasos De Origen"
        		android:textSize = "20sp"
        		android:layout_width="wrap_content" 
    			android:layout_height="wrap_content"
              	android:layout_alignParentLeft="true" 
    			android:layout_marginLeft="55px"
    			android:layout_marginTop ="10px"
        		android:layout_below = "@+id/relativeLayout2"      	
       		 />	
       		 
       		 
       		 
       		 <Button
        		android:id="@+id/terminaltrafficorigin"
        		android:text="Esperando Fuera Del Gate"
        		android:scaleType = "fitCenter"
        		android:layout_width="300px" 
    			android:layout_height="60px"
    			android:layout_alignParentLeft="true" 
    			android:layout_marginLeft="33px"
    			android:layout_marginTop ="35px"
        		android:layout_below = "@+id/textview3"
        		android:background = "@drawable/trip_button"
      		/>	
       		 
       		 
       		<Button
        		android:id="@+id/LocatingFreightOrigin"
        		android:text="Localizar El Transporte"
        		android:scaleType = "fitCenter"
        		android:layout_width="300px" 
    			android:layout_height="60px"
    			android:layout_alignParentLeft="true" 
    			android:layout_marginLeft="33px"
    			android:layout_marginTop ="35px"
        		android:layout_below = "@+id/terminaltrafficorigin"
        		android:background = "@drawable/trip_button"
        	/>  
        	
        	<Button
        		android:id="@+id/paperworkorigin"
        		android:text="Problema De Equipo"
        		android:scaleType = "fitCenter"
        		android:layout_width="300px" 
    			android:layout_height="60px"
    			android:layout_alignParentLeft="true" 
    			android:layout_marginLeft="33px"
    			android:layout_marginTop ="35px"
        		android:layout_below = "@+id/LocatingFreightOrigin"
        		android:background = "@drawable/trip_button"
        		
    		/> 

<!-- %&%&%&%&%&%&%&%&%&%&%&%&% ROAD DELAYS &%&%&%&%&%&%&%&%&%&%&%&%&%%&%&%&%&%&%&%&%&%&%&%& -->       		 
        	
         	
        	<TextView
